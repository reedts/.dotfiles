out_to_x no
own_window no
out_to_console yes
background no
update_interval 5.0


mpd_port 6600

TEXT
[
 { "full_text" : " ${cpu}%",
       "color" : ${if_match ${cpu} < 50}"\#9999AA"${else}"\#F92672"${endif}
 },

 { "full_text" : " ${memfree}",
       "color" : ${if_match ${memperc} < 80}"\#9999AA"${else}"\#F92672"${endif}
 },

 { "full_text" : ${if_up enp9s0}" ${addr enp9s0}",
       "color" : "\#86C00C"${else}"   ", "color" : "\#F92672"${endif}
 },

 { "full_text" : "${time %d-%m-%Y  %H:%M}",
       "color" : "\#9999AA"
 }
],
